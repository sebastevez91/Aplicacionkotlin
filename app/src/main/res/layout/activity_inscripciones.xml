<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InscripcionesActivity"
    android:background="@mipmap/fondokotlin">

    <TextView
        android:id="@+id/idTitleMisInc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Mis inscripciones"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/listIncripciones"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintVertical_bias="0.1" />

    <ListView
        android:id="@+id/listIncripciones"
        android:layout_width="322dp"
        android:layout_height="537dp"
        android:cacheColorHint="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/btnVInicio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_default="wrap"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idTitleMisInc"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintWidth_default="wrap" />

    <Button
        android:id="@+id/btnVInicio"
        android:layout_width="324dp"
        android:layout_height="57dp"
        android:backgroundTint="@color/blue"
        android:text="Volver a la pagina inicio"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listIncripciones" />

</androidx.constraintlayout.widget.ConstraintLayout>
